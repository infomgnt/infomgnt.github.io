	keywords = {data publication, data management, Computer and information sciences, data versioning},
	file = {Full Text PDF:/Users/pampel/Zotero/storage/MS7IWGFI/Klump - 2024 - Worshop Forschungsdaten-Publikationen zwischen Dy.pdf:application/pdf},
